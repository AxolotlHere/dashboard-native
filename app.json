{
  "expo": {
    "name": "dashboard",
    "slug": "dashboard",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "dashboard",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/FiraSans-MediumItalic.ttf"
          ],
          "android": {
            "fonts": [
              {
                "fontFamily": "Inter",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/Inter-Italic.ttf",
                    "weight": "700",
                    "style": "italic"
                  },
                  {
                    "path": "./assets/fonts/Inter.ttf",
                    "weight": "700"
                  }
                ]
              },
              {
                "fontFamily": "Epunda",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/EpundaSlab-Medium.ttf"
                  },
                  {
                    "path":"./assets/fonts/EpundaSlab-ExtraBold.ttf",
                    "weight":"700"
                  },
                  {
                    "path":"./assets/fonts/EpundaSlab-Medium.ttf",
                    "weight": "400"
                  }
                ]
              }
            ]
          },
          "ios": {
            "fonts": [
              "./assets/fonts/Inter.ttf",
              "./assets/fonts/Inter-Italic.ttf"
            ]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
